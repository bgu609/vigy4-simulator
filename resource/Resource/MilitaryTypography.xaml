    <FontWeight x:Key="FontWeight_Component_M">Medium</FontWeight>
    <FontWeight x:Key="FontWeight_Component_L">SemiBold</FontWeight>

    <Style x:Key="TextBlock_Headline"
           TargetType="TextBlock">
        <Style.Setters>
            <Setter Property="FontSize"
                    Value="{StaticResource FontSize_Headline}"/>
            <Setter Property="FontWeight"
                    Value="{StaticResource FontWeight_Headline}"/>
            <Setter Property="FontFamily"
                    Value="Noto Sans KR"/>
            <Setter Property="Foreground"
