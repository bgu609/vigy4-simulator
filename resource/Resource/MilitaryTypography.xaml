    <s:Double x:Key="FontSize_Txt_M">14.0</s:Double>
    <s:Double x:Key="FontSize_Txt_L">16.0</s:Double>
    <s:Double x:Key="FontSize_Txt_XL">32.0</s:Double>
    <s:Double x:Key="FontSize_Caption_S">8.0</s:Double>
