    <s:Double x:Key="FontSize_Component_M">14.0</s:Double>
    <s:Double x:Key="FontSize_Component_L">20.0</s:Double>

    <FontWeight x:Key="FontWeight_Headline">SemiBold</FontWeight>
    <FontWeight x:Key="FontWeight_BodyTitle">SemiBold</FontWeight>
    <FontWeight x:Key="FontWeight_SubTitle">SemiBold</FontWeight>
    <FontWeight x:Key="FontWeight_SubTitle_L">Medium</FontWeight>

    <FontWeight x:Key="FontWeight_Label_S">SemiBold</FontWeight>
    <FontWeight x:Key="FontWeight_Label_M">Medium</FontWeight>
    <FontWeight x:Key="FontWeight_List">Medium</FontWeight>
    <FontWeight x:Key="FontWeight_SubTxt">SemiBold</FontWeight>
    <FontWeight x:Key="FontWeight_Txt_S">SemiBold</FontWeight>
    <FontWeight x:Key="FontWeight_Txt_M">Medium</FontWeight>
    <FontWeight x:Key="FontWeight_Txt_L">SemiBold</FontWeight>
