                    xmlns:s="clr-namespace:System;assembly=mscorlib">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="MilitaryColor.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <s:Double x:Key="FontSize_Headline">15.0</s:Double>
    <s:Double x:Key="FontSize_BodyTitle">13.0</s:Double>
    <s:Double x:Key="FontSize_SubTitle">12.0</s:Double>
    <s:Double x:Key="FontSize_SubTitle_L">24.0</s:Double>

    <s:Double x:Key="FontSize_Label_S">12.0</s:Double>
