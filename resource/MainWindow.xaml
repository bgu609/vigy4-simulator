        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="1600" WindowStartupLocation="CenterScreen"
        Background="{StaticResource Brush_Popup640_Bg}">
    <Grid>
        <Grid.ColumnDefinitions>
