            <StackPanel Orientation="Horizontal"
                        Margin="10">
                <Button Style="{StaticResource MilitaryButton}"
                        Margin="5"
                        Content="Test"/>
                <Button Style="{StaticResource MilitaryButton}"
                        Margin="5"
                        Content="Test"
                        IsEnabled="False"/>

                <Button Style="{StaticResource MilitaryButton_Image}"
                        Grid.Row="0"
                        Grid.Column="2"
                        Margin="15, 0, 3, 0"
                        Width="26"
                        Height="26">
                    <Image Source="pack://application:,,,/Pa5455CmsResource;component/Images/icon_minus.png"
                           Width="12"
                           Height="12"/>
                </Button>
                <RepeatButton Style="{StaticResource MilitaryRepeatButton_Image}"
                              Grid.Row="0"
                              Grid.Column="6"
                              Margin="3, 0, 15, 0"
                              Width="26"
                              Height="26">
