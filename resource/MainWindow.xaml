                           VerticalAlignment="Center"/>
                            </Grid>
                        </RadioButton>
                    </StackPanel>
                </ScrollViewer>

                <ItemsControl Style="{StaticResource MilitaryItemControl}"
                      Grid.Row="2"
                              Margin="10"
                              Width="250"
                              HorizontalAlignment="Right">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>

                            <RadioButton Style="{StaticResource MilitaryRadioButton_TableRow}"
                                         Height="26">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="22"/>
                                    </Grid.ColumnDefinitions>

                                </Grid>
                            </RadioButton>

                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                </ItemsControl>
            </Grid>

        </StackPanel>

        <StackPanel Grid.Column="1"
                    Orientation="Vertical">

            <Border Style="{StaticResource MilitaryBorder_Popup}"
                    Width="640">
                <StackPanel Orientation="Vertical">

                    <Border Background="{StaticResource Brush_HeadlineBackground}">
                        <Grid>

                            <TextBlock Style="{StaticResource TextBlock_Headline}"
                                       Margin="10, 0"
                                       Text="Popup Headline"
                                       VerticalAlignment="Center"/>

                            <Button Style="{StaticResource MilitaryButton_Close}"
                                    HorizontalAlignment="Right"/>

                        </Grid>
                    </Border>

                    <Menu Background="Transparent">
                        <MenuItem Style="{StaticResource MilitaryMenuItem}" Header="Setting">
                            <MenuItem Header="Connection Setting" Command="{Binding showConnectionSettingDialog}"/>
                            <MenuItem Header="Radar Setting" Command="{Binding showRadarSettingDialog}"/>
                            <MenuItem Header="Laser Setting" Command="{Binding showLaserSettingDialog}"/>
                            <MenuItem Header="IR Setting" Command="{Binding showIrSettingDialog}"/>
                            <MenuItem Header="TV Setting" Command="{Binding showTvSettingDialog}"/>
                            <MenuItem Header="Video Tracking Setting" Command="{Binding showVideoTrackingSettingDialog}"/>
                        </MenuItem>
                        <MenuItem Style="{StaticResource MilitaryMenuItem}" Header="Control">
                            <MenuItem Header="Device Control" Command="{Binding showDeviceControlDialog}"/>
                            <MenuItem Header="Device Status" Command="{Binding showDeviceStatusControlDialog}"/>
                            <MenuItem Header="Delete Source Track" Command="{Binding showDeleteSourceTrackControlDialog}"/>
                            <MenuItem Header="Self-Cueing" Command="{Binding showSelfCueingControlDialog}"/>
                            <MenuItem Header="Ship Data Excluded Selection" Command="{Binding showShipDataExcludedSelectionControlDialog}"/>
                            <MenuItem Header="Radar Video Surveillance" Command="{Binding showRadarVideoSurveillanceControlDialog}"/>
                            <MenuItem Header="Request TRS Full Status"/>
                        </MenuItem>
                        <MenuItem Style="{StaticResource MilitaryMenuItem}" Header="Mode">
