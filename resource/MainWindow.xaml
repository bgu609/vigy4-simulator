                            <MenuItem Header="Tracking Mode" Command="{Binding showTrackingModeDialog}"/>
                            <MenuItem Header="Cancel Cueing Order"/>
                        </MenuItem>
                        <MenuItem Style="{StaticResource MilitaryMenuItem}" Header="Data">
                            <MenuItem Header="Autonomous Training Data" Command="{Binding showAutonomousTrainingDataDialog}"/>
                            <MenuItem Header="Estimated Anti Naval Mode Data" Command="{Binding showEstimatedAntiNavalModeDataDialog}"/>
                            <MenuItem Header="Ghost Trajectory Initial Data" Command="{Binding showGhostTrajectoryInitialDataDialog}"/>
                            <MenuItem Header="External Horn Global Test Data" Command="{Binding showExternalHornGlobalTestDataDialog}"/>
                            <MenuItem Header="Horizon Checks Data" Command="{Binding showHorizonChecksDataDialog}"/>
                            <MenuItem Header="Installation Data" Command="{Binding showInstallationDataDialog}"/>
                            <MenuItem Header="Logger Data" Command="{Binding showLoggerDataDialog}"/>
                            <MenuItem Header="Servo Test Data" Command="{Binding showServoTestDataDialog}"/>
                            <MenuItem Header="Test Gun Boresight Data" Command="{Binding showTestGunBoresightDataDialog}"/>
                        </MenuItem>
                        <MenuItem Style="{StaticResource MilitaryMenuItem}" Header="Track">
                        </MenuItem>
                        <MenuItem Style="{StaticResource MilitaryMenuItem}" Header="A-Scope">
                        </MenuItem>
