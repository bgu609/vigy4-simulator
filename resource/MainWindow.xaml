                <TreeViewItem Style="{StaticResource MilitaryTreeViewItem}"
                              Header="Item 1"
                              BorderBrush="{StaticResource Brush_TextBoxDisabled}"
                              Background="{StaticResource Brush_Warning}"
                              IsExpanded="True">
